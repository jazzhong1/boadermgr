Create table member(
    idx number primary key,
    member_id varchar2(10) not null,
    member_pwd varchar2(10) not null,
    member_name varchar2(20) not null,
    email varchar2(30),
    address varchar2(100),
    phone varchar2(20),
    enroll_date date
);
drop table member;

create table board(
idx number primary key,
div varchar2(20) check(div in('공지','일반','비밀')),
title varchar2(50) not null,
comments varchar2(3000) not null,
writer varchar2(20),
write_date date 
);

insert into board values(idx2.nextval,'공지','카네키','내용~~~~~~','홍성진','17/09/21');
