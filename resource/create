insert into board values(idx2.nextval,'비밀','정보보호개론','내용~~~~~~','메시','16/02/01');


drop table board;

create sequence idx1
start with 1
increment by 1
maxvalue 100
nocycle
;
drop sequence idx1;

create sequence idx2
start with 1
increment by 1
nocycle
;

drop sequence idx2;

